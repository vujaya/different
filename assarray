void main()
{
 int i,n,pro,sum,diff,a[5];
 printf("Enter the n value: ");
 scanf("%d",&n);
 pro=1;
 sum=diff=0;
 printf("Enter the numbers into array: ");
 for(i=0;i<n;i++)
 {
  scanf("%d",&a[i]);
 }
  for(i=0;i<n;i++)
  {
   pro*=a[i];
  }
  for(i=0;i<n;i++)
  {#include<stdio.h>

   sum+=a[i];
  }
 diff=pro-sum;
 printf("%d",diff);
}
